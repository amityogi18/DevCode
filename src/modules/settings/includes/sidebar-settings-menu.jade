script.
    function openNav() {
        document.getElementById("sideBarNav").style.width = "190px";
        document.getElementById("fixed-top").style.width = "0";
    }
    function closeNav() {
        document.getElementById("sideBarNav").style.width = "0";
        document.getElementById("fixed-top").style.width = "0";
    }
    function showNav() {
        document.getElementById("fixed-top").style.display = "block";
        document.getElementById("fixed-iconMenu").style.display = "none";
    }
    function hideNav() {
        document.getElementById("fixed-top").style.display = "none";
        document.getElementById("fixed-iconMenu").style.display = "block";
    }
.sideMenuSection
    .col-md-1.pull-right.hidden-lg.hidden-md.hidden-sm(ng-if="!$root.mobile")
      i#open-icon.fa.fa-ellipsis-v.fa-2x(onclick='openNav()')
    .col-md-1.pull-right.hidden-lg.hidden-md.hidden-sm(ng-if="$root.mobile")
      i#open-icon.fa.fa-bars(onclick='openNav()')   
    .navigation-container
      #fixed-iconMenu.nav-side-menu.navbar-fixed-top.icon-menu
        .menu-list.hidden-xs
            ul#menu-content.menu-content
              li.menu-color(onclick="showNav()")
                md-button(aria-label='Open menu', uib-tooltip='Open Menu', tooltip-placement='bottom')
                  md-icon(md-font-set='fa') &#xf233
              li(ui-sref-active="active")
                a(data-ui-sref='settings.general-settings', uib-tooltip='General')
                  md-button(aria-label='General')
                    md-icon(md-font-set='fa') &#xf108 
              li(ui-sref-active="active")
                a(data-ui-sref='settings.notification-settings', uib-tooltip='Notifications')
                  md-button(aria-label='Notifications')
                    md-icon(md-font-set='fa') &#xf0a2 
              li(ui-sref-active="active", hidden)
                a(data-ui-sref='settings.email-template-settings', uib-tooltip='Email Template', data-ng-show='user.userRoles == 1 || user.userRoles == 2 || user.userRoles == 6')
                  md-button(aria-label='Email Template')
                    md-icon(md-font-set='fa') &#xf128 
              li(ui-sref-active="active")
                a(data-ui-sref='settings.custom-question', uib-tooltip='Custom Questions', data-ng-show='user.userRoles == 1 || user.userRoles == 2 || user.userRoles == 6')
                  md-button(aria-label='Custom Questions')
                    md-icon(md-font-set='fa') &#xf128
              li(ui-sref-active="active", hidden)
                a(data-ui-sref='settings.theme-settings', uib-tooltip='Theme', data-ng-hide='user.userRoles == 14')
                  md-button(aria-label='Theme')
                    md-icon(md-font-set='fa') &#xf2b2
              li(data-ng-hide='user.userRoles == 7', ui-sref-active="active")
                a(data-ui-sref='settings.issue-ticketing', uib-tooltip='Issue Ticketing', data-ng-hide='user.userRoles == 14 || user.userRoles == 13 || user.userRoles == 12 || user.userRoles == 9 || user.userRoles == 11 || user.userRoles == 10')
                  md-button(aria-label='Issue Ticketing') 
                    md-icon(md-font-set='fa') &#xf145 
              li(ui-sref-active="active")
                a(data-ui-sref='settings.calendar', uib-tooltip='Calendar',data-ng-hide='user.userRoles == 7 || user.userRoles == 2 || user.userRoles == 14 || user.userRoles == 13 || user.userRoles == 12 || user.userRoles == 9 || user.userRoles == 11 || user.userRoles == 10')
