                  a(data-ui-sref='settings.admin-department' , data-ng-show='user.userRoles == 1 || user.userRoles == 2 || user.userRoles == 6')
                    md-button(aria-label='Departments')
                        md-icon(md-font-set='fa') &#xf19d
                    span.text-of-sidebar(data-translate='DEPARTMENTS')
                li.sideBarMenuIcon(ui-sref-active="active")
                  a(data-ui-sref='settings.admin-user' , data-ng-show='user.userRoles == 1 || user.userRoles == 2')
                    md-button(aria-label='Users')
                        md-icon(md-font-set='fa') &#xf007
                    span.text-of-sidebar(data-translate='USERS')
                li.sideBarMenuIcon(ui-sref-active="active")
                  a(data-ui-sref='settings.position-automation' , data-ng-show='user.userRoles == 1 || user.userRoles == 2')
                    md-button(aria-label='Position Automation')
                        md-icon(md-font-set='fa') &#xf140
                    span.text-of-sidebar(data-translate='POSITION-AUTOMATION')
                li.sideBarMenuIcon(ui-sref-active="active", data-ng-show='user.userRoles == 1')
                  a(data-ui-sref='settings.admin-plan')
                    md-button(aria-label='My Plan')
                        md-icon(md-font-set='fa') &#xf12e
                    span.text-of-sidebar(data-translate='MY-PLAN')
                li.sideBarMenuIcon(ui-sref-active="active", data-ng-show='user.userRoles == 1')
                  a(data-ui-sref='settings.admin-payment-transaction')
                    md-button(aria-label='Transaction History')
                        md-icon(md-font-set='fa') &#xf0d6
                    span.text-of-sidebar(data-translate='TRANSACTION-HISTORY')
