                    span.text-of-sidebar(data-translate='ISSUE-TICKETING')
                li.sideBarMenuIcon(ui-sref-active="active")
                  a(data-ui-sref='settings.calendar' , data-ng-hide='user.userRoles == 7 || user.userRoles == 2 || user.userRoles == 14 || user.userRoles == 13 || user.userRoles == 12 || user.userRoles == 9 || user.userRoles == 11 || user.userRoles == 10')
