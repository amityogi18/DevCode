                    md-icon(md-font-set='fa') &#xf133      
              li(ui-sref-active="active")
                a(data-ui-sref='settings.admin-company-info',  uib-tooltip='Company Information' , data-ng-show='user.userRoles == 1 || user.userRoles == 2 || user.userRoles == 6')
                  md-button(aria-label='Company Information')
                    md-icon(md-font-set='fa') &#xf129 
              li(ui-sref-active="active")
                a(data-ui-sref='settings.admin-department', uib-tooltip='Departments' , data-ng-show='user.userRoles == 1 || user.userRoles == 2 || user.userRoles == 6')
                  md-button(aria-label='Departments')
                    md-icon(md-font-set='fa') &#xf19d
              li(ui-sref-active="active")
                a(data-ui-sref='settings.admin-user' , uib-tooltip='Users',data-ng-show='user.userRoles == 1 || user.userRoles == 2')
                  md-button(aria-label='Users')
                    md-icon(md-font-set='fa') &#xf007 
              li(ui-sref-active="active")
                a(data-ui-sref='settings.position-automation' , uib-tooltip='Position Automation', data-ng-show='user.userRoles == 1 || user.userRoles == 2')
                  md-button(aria-label='Position Automation')
                    md-icon(md-font-set='fa') &#xf140
              li(ui-sref-active="active", data-ng-show='user.userRoles == 1')
                a(data-ui-sref='settings.admin-plan', ng-class="{active:$state.includes('settings.admin-payment-plan') || $state.includes('settings.admin-confirmation') || $state.includes('settings.admin-payment')}", uib-tooltip='My Plan')
                  md-button(aria-label='My Plan')
                    md-icon(md-font-set='fa') &#xf12e 
              li(ui-sref-active="active", data-ng-show='user.userRoles == 1')
                a(data-ui-sref='settings.admin-payment-transaction', uib-tooltip='Transaction History')
                  md-button(aria-label='Transaction History')
                    md-icon(md-font-set='fa') &#xf0d6 
