.content-wrapper.super_admin.clients
  .row
    .col-xs-12.col-sm-12.col-md-12
      .client
        span.client-heading All Job Portals
  .panel.panel-default
    .panel-body
      .row
         div(ng-cloak='')
            md-content
              md-tabs(md-dynamic-height='', md-border-bottom='')
                md-tab(label='Posted Job Portal')
                  md-content.md-padding
                    h1.md-display-2
                    .col-sm-4.col-md-4.col-xs-4(ng-repeat='product in allJobPortalSaCtrl.portalList' ng-if='product.isJobposted')
                        .job-board
                          .site-img(style='text-align: center; padding: 20px; box-shadow: 0px 0px 15px #a6a6a6; margin-top:10px;')
                            a(data-ui-sref='sa.position-review({portalId:product.portalId})')
                              img(width="80", height="40", ng-src="{{product.portalImagePath?product.portalImagePath:'/images/times.jpg'}}", alt='',uib-tooltip="Click Here to View the Details")
                   
