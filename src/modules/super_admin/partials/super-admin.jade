.content-wrapper.super_admin.dashboard
  .row
    .col-md-12
      .dashboard
        span.dashboard-heading(data-translate="DASHBOARD")
  .panel.panel-default
    .panel-heading.dashboard-profile-heading Candidate Profile
    .panel-body
      .row
        .col-md-4.col-sm-12.col-xs-12
          md-card.super-panel-card
            md-card-header.super-panel-header
              md-card-header-text  
                span.md-title.md-title-font(data-translate="2-MIN-INTRO-STATICS")
            .panel-body.candidate-profile-panel
                table.table
                  tbody
                    tr
                      td New Signup
                      td
                        .mydiv
                          span {{superadminCtrl.twoMinStaticsData.newSignUp}}
                    tr
                      td No. of Intro Recorded
                      td
                        .mydiv
                           span {{superadminCtrl.twoMinStaticsData.noOfIntroRecorded}}
                    tr
                      td No. Of Completed Interview
                      td
                        .mydiv
                          span {{superadminCtrl.twoMinStaticsData.noOfTestTaken}}
                    tr
                      td Not Logged Since Last One Month
                      td
                        .mydiv
                          span {{superadminCtrl.twoMinStaticsData.notLoggedInSinceLastOneMonth}}
                    tr
                      td No. Of Certified Candidates
                      td
                        .mydiv
                          span {{superadminCtrl.twoMinStaticsData.certifiedCandidates}}
        .col-md-4.col-sm-12.col-xs-12
          md-card.super-panel-card
            md-card-header.super-panel-header
              md-card-header-text
                span.md-title.md-title-font(data-translate="COMPANY-CANDIDATE-STATICS")
            .panel-body.candidate-profile-panel
              table.table
                tbody
                  tr
                    td Total Company
                    td
                      .mydiv
                        span {{superadminCtrl.companyCandidateList.companyCount}}
                  tr
                    td Total Company Candidate
                    td
                      .mydiv
                        span {{superadminCtrl.companyCandidateList.companyCandidateCount}}
                  tr
                    td Total Live Interview Candidate
                    td
                      .mydiv
                        span {{superadminCtrl.companyCandidateList.interviewCandidateCountLive}}
                  tr
                    td Total Live Now Interview Candidate
                    td
                      .mydiv
                        span {{superadminCtrl.companyCandidateList.interviewCandidateCountLivenow}}
                  tr
                    td Total OnDemand Interview Candidate
                    td
                      .mydiv
                        span {{superadminCtrl.companyCandidateList.interviewCandidateCountOndemand}}
                  tr
                    td Total Onsite Interview Candidate
                    td
                      .mydiv
                        span {{superadminCtrl.companyCandidateList.interviewCandidateCountSite}}
        .col-md-4.col-sm-12.col-xs-12
          md-card.super-panel-card
            md-card-header.super-panel-header
              md-card-header-text
                span.md-title.md-title-font Social Media Signup
            .panel-body.candidate-profile-panel
              table.table
                tbody
                  tr
                    td Signup With Gmail
                      span
                        i.fa.fa-google-plus-square.social-net-icon-gmail(aria-hidden='true')  
                    td
                      .mydiv
                        span {{superadminCtrl.profileSocialList.noOfEmail}}
                  tr
                    td Signup With LinkedIn
                      span
                        i.fa.fa-linkedin-square.social-net-icon-linkdin(aria-hidden='true')
                    td
                      .mydiv
                        span {{superadminCtrl.profileSocialList.noOfLinkdin}}
                  tr
                    td Signup With Twitter
                      span
                        i.fa.fa-twitter-square.social-net-icon-twitter(aria-hidden='true')
                    td
                      .mydiv
                        span {{superadminCtrl.profileSocialList.noOfTwitter}}
                  tr
                    td Signup With Facebook
                      span
                        i.fa.fa-facebook-square.social-net-icon-facebook(aria-hidden='true')
                    td
                      .mydiv
                        span {{superadminCtrl.profileSocialList.noOffacebook}}
                    tr
                      td Signup With Others
                        span
                          i.fa.fa-link.social-net-icon-facebook(aria-hidden='true')
                      td
                        .mydiv
                          span {{superadminCtrl.profileSocialList.noOfOthers}}
  .panel.panel-default
    .panel-heading.dashboard-profile-heading Interview & Conference
    .panel-body
      .row
        .col-md-6.col-sm-12.col-xs-12
          md-card.super-panel-card
            md-card-header.super-panel-header
