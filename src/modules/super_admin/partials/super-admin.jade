                span.md-title.md-title-font(data-translate="CLIENT-STATICS")
            .panel-body.interview-conference-panel
              table.table
                tbody
                  tr
                    td New Signup
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.newSignUp}}
                  tr
                    td Active
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.active}}
                  tr
                    td Inactive
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.inactive}}
                  tr
                    td Re-Activated
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.reactived}}
                  tr
                    td Not Logged Since Last One Month
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.notLoggedInSinceOneMonth}}
                  tr
                    td No Of Free Conference User
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.noOfFreeConferenceUser}}
                  tr
                    td No Of Free Interview User
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.noOfFreeInterviewUser}}
                  tr
                    td No Of Paid Conference User
                    td
                      .mydiv
                        span {{superadminCtrl.clientList.noOfpaidConferenceUser}}
