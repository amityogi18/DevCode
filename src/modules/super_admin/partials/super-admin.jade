          md-card.super-panel-card
            md-card-header.super-panel-header
              md-card-header-text
                span.md-title.md-title-font(data-translate="STATUS")
            .panel-body.interview-conference-panel
              table.table
                tbody
                  tr
                    td Total Users
                    td
                      .mydiv
                        span {{superadminCtrl.statusCountList.totalUsers}}
                  tr
                    td Total Candidate
                    td
                      .mydiv
                        span {{superadminCtrl.statusCountList.totalCandidates}}
                  tr
                    td Audio Records
                    td
                      .mydiv
                        span {{superadminCtrl.statusCountList.audioRecords}}
                  tr
                    td Video Records
                    td
                      .mydiv
                        span {{superadminCtrl.statusCountList.videoRecords}}
                  tr
                    td Guest
                    td
                      .mydiv
                        span {{superadminCtrl.statusCountList.guestUsers}}
      .row
        .col-md-6.col-sm-12.col-xs-12
