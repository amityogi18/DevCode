                        label.col-md-4.col-sm-4.col-xs-6.space-right Currency                     
                        .col-md-8.col-sm-8.col-xs-6 
                          p(ng-if='viewPositionCtrl.position.currencyType') {{viewPositionCtrl.position.currencyType}}
                          p(ng-if='viewPositionCtrl.position.currencyType == null') N/A
                        label.col-md-4.col-sm-4.col-xs-6.space-right Salary                      
                        .col-md-8.col-sm-8.col-xs-6 
                          p(ng-if='viewPositionCtrl.position.salary') {{viewPositionCtrl.position.salary}}
                          p(ng-if='viewPositionCtrl.position.salary == null') N/A
                      .col-md-6.col-sm-6.col-xs-12
                        label.col-md-4.col-sm-4.col-xs-6.space-right Education                 
                        .col-md-8.col-sm-8.col-xs-6 
                          p(ng-if='viewPositionCtrl.position.requiredQualification') {{viewPositionCtrl.position.requiredQualification}}
                          p(ng-if='viewPositionCtrl.position.requiredQualification == null') N/A
                        label.col-md-4.col-sm-4.col-xs-6.space-right Position Share                 
