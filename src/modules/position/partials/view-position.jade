                        span.label.label-danger(data-ng-if='viewPositionCtrl.applicationFormData.email == 3') Off
                        span.label.label-info(data-ng-if='viewPositionCtrl.applicationFormData.email == 0') N/A
                      p  
                        span.label.label-primary(data-ng-if='viewPositionCtrl.applicationFormData.phoneNumber == 1') Required
                        span.label.label-warning(data-ng-if='viewPositionCtrl.applicationFormData.phoneNumber == 2') Optional
                        span.label.label-danger(data-ng-if='viewPositionCtrl.applicationFormData.phoneNumber == 3') Off
                        span.label.label-info(data-ng-if='viewPositionCtrl.applicationFormData.phoneNumber == 0') N/A
                      p  
