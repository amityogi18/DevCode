                        label.col-md-4.col-sm-4.col-xs-6.space-right Remote Location                      
                        .col-md-8.col-sm-8.col-xs-6 
                          p(ng-if='viewPositionCtrl.position.remoteLocation')
                            |{{viewPositionCtrl.position.remoteLocation}}
                          p(ng-if='!viewPositionCtrl.position.remoteLocation')
                            | N/A
                .row.main-bodyclr.application-form-section
                  .col-md-4.col-sm-4.col-xs-12
                    h4
                      i.fa.fa-anchor
                      | Custom Option
                    div.alert.alert-info(ng-show='viewPositionCtrl.position.additionalOptions.customOptions[0].customOption ===""')
                      p No custom option added yet! 
                    table.table.table-bordered(ng-hide='viewPositionCtrl.position.additionalOptions.customOptions[0].customOption ===""')
                      thead
                        tr
                          th.common-header(colspan="2") Custom Field
                      tbody
                        tr(ng-if='data.customOption !== ""', ng-repeat='data in viewPositionCtrl.position.additionalOptions.customOptions')
                          td
                            strong Custom Field {{$index + 1}}
                          td               
                            span(ng-if='data.customOption !== ""') {{data.customOption}}


                  .col-md-4.col-sm-4.col-xs-12
                    h4
                      i.fa.fa-star(area-hidden='true')
                      |Insight Rating
                    div.alert.alert-info(ng-show='viewPositionCtrl.position.insightCriteria.criterias[0].criteria === ""')
                      p No insight criteria added yet!  
                    table.table.table-bordered(ng-hide='viewPositionCtrl.position.insightCriteria.criterias[0].criteria === ""')
                      thead
                        tr
                          th Criteria
                          th Is Mandatory
                      tbody
                        tr(ng-if='data.criteria !== ""', ng-repeat='data in viewPositionCtrl.position.insightCriteria.criterias')
                          td                
                            span(ng-if='data.criteria !== ""') {{data.criteria}}
                          td
                            span(ng-if='data.isMandatory == 0') No
                            span(ng-if='data.isMandatory !== 0') Yes

                  .col-md-4.col-sm-4.col-xs-12
                    h4
                      i.fa.fa-paper-plane-o(area-hidden='true')
                      |Advance Criteria
                    div.alert.alert-info(ng-show='viewPositionCtrl.position.advanceCriteria.criterias[0].criteria === ""')
                       p No advance criteria added yet!   
                    table.table.table-bordered(ng-hide='viewPositionCtrl.position.advanceCriteria.criterias[0].criteria === ""')
                      thead
                        tr
                          th Criteria
                          th Is Mandatory
                      tbody
                        tr(ng-if='data.criteria !== ""', ng-repeat='data in viewPositionCtrl.position.advanceCriteria.criterias')
                          td                
                            span(ng-if='data.criteria !== ""') {{data.criteria}}
                          td
                            span(ng-if='data.isMandatory == 0') No
                            span(ng-if='data.isMandatory !== 0') Yes           

                .row.main-bodyclr.application-form-section 
                  h4 
                    i.fa.fa-rocket
                    |Application Form
