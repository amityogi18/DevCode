                      p  
                        span.label.label-primary(data-ng-if='viewPositionCtrl.applicationFormData.resume == 1') Required
                        span.label.label-warning(data-ng-if='viewPositionCtrl.applicationFormData.resume == 2') Optional
                        span.label.label-danger(data-ng-if='viewPositionCtrl.applicationFormData.resume == 3') Off
                        span.label.label-info(data-ng-if='viewPositionCtrl.applicationFormData.resume == 0') N/A            
                .row.main-bodyclr.portal-section
                  h4
                    i.fa.fa-shopping-bag(area-hidden='true')
                    |Portal Information
                  .col-md-6.col-sm-6.col-xs-12  
                    table.table(data-ng-if="viewPositionCtrl.jobPortalsData.length > 1")
                      thead
                        tr 
                          th Portal Name
                          th Status
                          th Comment
                      tbody
                        tr(ng-repeat='portals in viewPositionCtrl.jobPortalsData')
                          td {{portals.portalName}}
                          td {{portals.status}}
                          td
                            span(ng-if="portals.comment === null") N/A
