.content-wrapper.view-position
  .col-md-12.col-sm-12.col-xs-12(data-ng-if='!viewPositionCtrl.hideView') 
    .row
      h1.col-md-6.col-sm-12.col-xs-12.panel-title {{viewPositionCtrl.position.name}} (View)
      .col-md-6.col-sm-12.col-xs-12
        .pull-right.hidden-xs
          md-button.md-raised.md-primary(aria-label='ACTIVATE',type='button', data-toggle='modal', data-target='#activateViewModal', data-ng-if='viewPositionCtrl.position.statusId !== 1', data-ng-hide="viewPositionCtrl.position.statusId === 3") Activate
          md-button.md-raised.md-warn(aria-label='EDIT',type='button', data-ui-sref='app.update-position({ positionId: viewPositionCtrl.positionId })',data-ng-if='user.userRoles !== 3', data-ng-hide="viewPositionCtrl.position.statusId === 3") Edit
          md-button.md-raised.md-primary(aria-label='BACK',type='button', data-ui-sref='app.position') Back
        .pull-right.act-back-btn.visible-xs
          a.edit-back.btn-green(type='button', data-toggle='modal', data-target='#activateViewModal', data-ng-if='viewPositionCtrl.position.statusId !== 1')
            i.fa.fa-check.fa-2x.popbtn_cross(data-toggle='tooltip', data-placement='top', title='cancel', aria-hidden='true')
          a.edit-back.btn-primary(type='button', data-ui-sref='app.update-position({ positionId: viewPositionCtrl.positionId })',data-ng-if='user.userRoles !== 3')
            i.fa.fa-pencil-square-o.fa-2x.popbtn_cross(data-toggle='tooltip', data-placement='top', title='cancel', aria-hidden='true')
          a.btn-green(type='button', data-ui-sref='app.position') 
            i.fa.fa-arrow-left.fa-2x.popbtn_cross(data-toggle='tooltip', data-placement='top', title='cancel', aria-hidden='true')
    #activateViewModal.modal.fade(role='dialog')
      .modal-dialog
        .modal-content.content-of-modal
          .modal-header
            | Activate Position
            button.close(type='button', data-dismiss='modal') Ã—
            h4.modal-title
          .modal-body.body-of-cp-modal
            p.discard-msg Are you sure you want to Activate the Position ?
          .modal-footer.footer-of-cp-modal
            md-button.md-raised.md-primary(aria-label='ACTIVATE',type='button', data-dismiss='modal', data-ng-click="viewPositionCtrl.activatePosition()") Activate
            md-button.md-raised.canceldata-btn(aria-label='cancel',type='button', data-dismiss='modal', data-translate='CANCEL') Cancel
    .panel-body
      div(ng-cloak='')
        md-content
          md-tabs(md-dynamic-height='', md-border-bottom='')
            md-tab(label=' Position ')
              md-content.md-padding        
                .main-body-wrapper
                  .default-form
                    .col-md-12.col-sm-12.col-xs-12.spacing-off.position-detail-section
                      .col-md-12.col-sm-12.col-xs-12
                        h4
                          i.fa.fa-briefcase(area-hidden='true')
                          |Position Details
                      .first-form.input-section
                        .col-md-6.col-sm-6.col-xs-12.spacing-off
                          label.col-md-4.col-sm-4.col-xs-6.space-right Position Code :
                          .col-md-8.col-sm-8.col-xs-6
                            p {{viewPositionCtrl.position.code}}
                          label.col-md-4.col-sm-4.col-xs-6.space-right Position Title :
                          .col-md-8.col-sm-8.col-xs-6
                            p {{viewPositionCtrl.position.name}}
                          label.col-md-4.col-sm-4.col-xs-6.space-right Recruiter Name :
                          .col-md-8.col-sm-8.col-xs-6
                            p {{viewPositionCtrl.position.recruiterName}}
                          label.col-md-4.col-sm-4.col-xs-6.space-right Department :
                          .col-md-8.col-sm-8.col-xs-6.departmen
                            p {{viewPositionCtrl.departmentNameId.name}}
                          label.col-md-4.col-sm-4.col-xs-6.space-right Location :
                          .col-md-8.col-sm-8.col-xs-6
                            p {{viewPositionCtrl.position.additionalOptions.address}}
                              //span(ng-if='viewPositionCtrl.position.additionalOptions.country == null && viewPositionCtrl.position.additionalOptions.state == null && viewPositionCtrl.position.additionalOptions.city == null ') N/A
                              //span(ng-if='viewPositionCtrl.position.additionalOptions.country !== null && viewPositionCtrl.position.additionalOptions.state !== null && viewPositionCtrl.position.additionalOptions.city !== null ')
                                | {{viewPositionCtrl.countrySetName.countryName}}, {{viewPositionCtrl.stateSetName.stateName}}, {{viewPositionCtrl.citySetName.cityName}}
                          label.col-md-4.col-sm-4.col-xs-6.space-right Experience(years) :
                          .col-md-8.col-sm-8.col-xs-6 
                            p
                              span(ng-if='viewPositionCtrl.position.additionalOptions.experienceFrom == 0 && viewPositionCtrl.position.additionalOptions.experienceTo == 0') N/A
                              span(ng-if='viewPositionCtrl.position.additionalOptions.experienceFrom !== 0 && viewPositionCtrl.position.additionalOptions.experienceTo !== 0')
                              | {{viewPositionCtrl.position.additionalOptions.experienceFrom}} To {{viewPositionCtrl.position.additionalOptions.experienceTo}}      
                        .col-md-6.col-sm-6.col-xs-12.spacing-off
                          label.col-md-4.col-sm-4.col-xs-6.space-right Primary Skill :
                          .col-md-8.col-sm-8.col-xs-6
                            p(ng-if='viewPositionCtrl.primarySkillName.skillsetName') {{viewPositionCtrl.primarySkillName.skillsetName}}
                            p(ng-if='!viewPositionCtrl.primarySkillName.skillsetName') N/A
                          label.col-md-4.col-sm-4.col-xs-6.space-right Secondary Skill :
                          .col-md-8.col-sm-8.col-xs-6
                            p(ng-if='viewPositionCtrl.secondrySkillName.skillsetName') {{viewPositionCtrl.secondrySkillName.skillsetName}}
                            p(ng-if='!viewPositionCtrl.secondrySkillName.skillsetName') N/A
                          label.col-md-4.col-sm-4.col-xs-6.space-right Tertiary Skill :
                          .col-md-8.col-sm-8.col-xs-6
                            p(ng-if='viewPositionCtrl.tertiarySkillName.skillsetName') {{viewPositionCtrl.tertiarySkillName.skillsetName}}
                            p(ng-if='!viewPositionCtrl.tertiarySkillName.skillsetName') N/A
                          label.col-md-4.col-sm-4.col-xs-6.space-right Position Expiry Date :
                          .col-md-8.col-sm-8.col-xs-6
                            p(ng-if="viewPositionCtrl.position.additionalOptions.positionExpiryDate == '0000-00-00 00:00:00' || viewPositionCtrl.position.additionalOptions.positionExpiryDate === null") N/A
                            p(ng-if="viewPositionCtrl.position.additionalOptions.positionExpiryDate !== '0000-00-00 00:00:00' && viewPositionCtrl.position.additionalOptions.positionExpiryDate !== null") {{viewPositionCtrl.position.additionalOptions.positionExpiryDate | date : "MM-dd-yyyy"}} 
                          label.col-md-4.col-sm-4.col-xs-6.space-right Other Skills :
                          .col-md-8.col-sm-8.col-xs-6
                            p
                              span(ng-if='viewPositionCtrl.position.additionalOptions.otherSkills == ""') N/A
                              span(ng-if='viewPositionCtrl.position.additionalOptions.otherSkills !== ""') {{viewPositionCtrl.position.additionalOptions.otherSkills}}
                .main-body-wrapper
                  .default-form
